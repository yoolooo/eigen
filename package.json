  "lint-staged": {
    "*.@(ts|tsx)": [
      "tslint -c tslint.json -p tsconfig.json --fix",
      "yarn prettier-write",
      "node scripts/strictness-migration.js check-staged"
    ],
    "*.@(json|md)": [
      "yarn prettier-write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "husky": {
    "hooks": {
      "post-merge": "pull-lock",
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  },
